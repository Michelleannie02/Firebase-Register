# platform :ios, '9.0'

target 'Firebase Register' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Firebase Register

    pod 'Firebase/Core'
    pod 'Firebase/Auth'
    pod 'Firebase/Firestore'
    pod 'Firebase/Storage'
    pod 'Firebase/Database'
    pod 'ProgressHUD'

end
